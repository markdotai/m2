<settings>

    <setting propertyKey="@Properties.EP" title="@Strings.ExportProfileTitle">
        <settingConfig type="alphaNumeric" maxLength="1024" />
    </setting>

    <setting propertyKey="@Properties.PM" title="@Strings.ProfileManagementTitle">
        <settingConfig type="list">
            <listEntry value="0">@Strings.ProfileCurrent</listEntry>
            <listEntry value="1">@Strings.ProfileLoad</listEntry>
            <listEntry value="2">@Strings.ProfileSave</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.PN" title="@Strings.ProfileNumberTitle">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.PD" title="@Strings.ProfileDaysTitle">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.PS" title="@Strings.ProfileStartTimeTitle">
        <settingConfig type="alphaNumeric" maxLength="20" />
    </setting>

    <setting propertyKey="@Properties.PE" title="@Strings.ProfileEndTimeTitle">
        <settingConfig type="alphaNumeric" maxLength="20" />
    </setting>

    <setting propertyKey="@Properties.35" title="@Strings.GlanceProfileTitle">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.PB" title="@Strings.ProfileBlockTitle">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.PR" title="@Strings.ProfileRandomTitle">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.DP" title="@Strings.DemoProfilesTitle">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.DR" title="@Strings.DemoProfilesRangeTitle">
        <settingConfig type="alphaNumeric" maxLength="20" />
    </setting>

    <setting propertyKey="@Properties.SA" title="@Strings.SunAltitudeAdjustTitle">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.NC" title="@Strings.NonActiveCalorieOverrideTitle">
        <settingConfig type="numeric" />
    </setting>

    <setting propertyKey="@Properties.H" title="@Strings.HonestyTitle">
        <settingConfig type="boolean" />
    </setting>

</settings>
